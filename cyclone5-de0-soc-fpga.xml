<?xml version="1.0" encoding="UTF-8"?>
<!--
      remote="poky"
      revision="refs/heads/master"
      sync-j="4" />

-->
<manifest>
  <remote
      name="poky"
      fetch="https://git.yoctoproject.org/git" />
  <remote
      name="oe"
      fetch="https://github.com/openembedded" />
  <remote
      name="altera"
      fetch="https://github.com/kraj" />

  <default
      remote="poky"
      revision="refs/heads/master"
      sync-j="4"
      sync-tags="true" />

  <project
      name="poky"
      path="poky"
      revision="dunfell"
      upstream="dunfell"
      remote="poky" />
  <project
      name="meta-openembedded"
      path="poky/meta-openembedded"
      revision="dunfell"
      upstream="dunfell"
      remote="oe" />
  <project
      name="meta-altera"
      path="poky/meta-altera"
      upstream="master"
      remote="altera" />
</manifest>
